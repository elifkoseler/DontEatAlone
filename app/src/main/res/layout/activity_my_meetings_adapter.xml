<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    android:backgroundTint="#FFFFFF">

    <Button
        android:id="@+id/editButton"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="22dp"
        android:background="#BEA2DF"
        android:backgroundTint="#FFFFFF"
        android:text="EdÄ±t"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/editdatetimeText"
        app:layout_constraintStart_toEndOf="@+id/editdatetimeText"
        app:layout_constraintTop_toBottomOf="@+id/editRestaurantText"
        tools:ignore="MissingConstraints" />


    <ImageView
        android:id="@+id/editImageView"
        android:layout_width="409dp"
        android:layout_height="0dp"
        android:layout_marginBottom="2dp"
        android:padding="10dp"
        app:layout_constraintBottom_toTopOf="@+id/editDistrictText"
        app:layout_constraintEnd_toEndOf="@+id/editdatetimeText"
        app:layout_constraintStart_toEndOf="@+id/editdatetimeText"
        app:layout_constraintTop_toBottomOf="@+id/editmeetingNameText"
        app:srcCompat="@drawable/ic_launcher_foreground"
        tools:ignore="MissingConstraints" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="414dp"
        android:layout_height="376dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/editdatetimeText"
        app:layout_constraintStart_toEndOf="@+id/editdatetimeText"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">


    </LinearLayout>

    <TextView
        android:id="@+id/editDistrictText"
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:layout_marginEnd="1dp"
        android:text="district"
        android:textAlignment="center"
        android:textColor="#311B92"
        android:textSize="16sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/editRestaurantText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/editdatetimeText"
        app:layout_constraintTop_toBottomOf="@+id/editImageView"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/editRestaurantText"
        android:layout_width="409dp"
        android:layout_height="32dp"
        android:layout_marginBottom="6dp"
        android:text="restaurantname"
        android:textAlignment="center"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/editButton"
        app:layout_constraintEnd_toEndOf="@+id/editdatetimeText"
        app:layout_constraintStart_toEndOf="@+id/editdatetimeText"
        app:layout_constraintTop_toBottomOf="@+id/editDistrictText"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/editmeetingNameText"
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:text="meetingname"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/editImageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view3"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/editdatetimeText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:text="datetime"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/editRestaurantText"
        app:layout_constraintEnd_toStartOf="@+id/editDistrictText"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/editDistrictText"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/view3"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginBottom="1dp"
        android:background="@android:color/darker_gray"
        app:layout_constraintBottom_toTopOf="@+id/editmeetingNameText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>