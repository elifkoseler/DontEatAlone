<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    android:backgroundTint="#FFFFFF">

    <Button
        android:id="@+id/button10"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="21dp"
        android:background="#BEA2DF"
        android:backgroundTint="#FFFFFF"
        android:text="joÄ±n"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/datetimeText"
        app:layout_constraintStart_toEndOf="@+id/datetimeText"
        app:layout_constraintTop_toBottomOf="@+id/restaurantText"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/recycler_row_imageView"
        android:layout_width="409dp"
        android:layout_height="200dp"
        android:layout_marginBottom="1dp"
        android:padding="10dp"
        app:layout_constraintBottom_toTopOf="@+id/districtText"
        app:layout_constraintEnd_toEndOf="@+id/datetimeText"
        app:layout_constraintStart_toEndOf="@+id/datetimeText"
        app:layout_constraintTop_toBottomOf="@+id/meetingNameText"
        app:srcCompat="@drawable/ic_launcher_background"
        tools:ignore="MissingConstraints" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="410dp"
        android:layout_height="375dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/datetimeText"
        app:layout_constraintStart_toEndOf="@+id/datetimeText"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints"></LinearLayout>

    <TextView
        android:id="@+id/districtText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="1dp"
        android:text="district"
        android:textAlignment="center"
        android:textColor="#311B92"
        android:textSize="16sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/restaurantText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/datetimeText"
        app:layout_constraintTop_toBottomOf="@+id/recycler_row_imageView"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/restaurantText"
        android:layout_width="409dp"
        android:layout_height="0dp"
        android:layout_marginBottom="6dp"
        android:text="restaurantname"
        android:textAlignment="center"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/button10"
        app:layout_constraintEnd_toEndOf="@+id/datetimeText"
        app:layout_constraintStart_toEndOf="@+id/datetimeText"
        app:layout_constraintTop_toBottomOf="@+id/districtText"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/meetingNameText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:text="meetingname"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/recycler_row_imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/datetimeText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:text="datetime"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/restaurantText"
        app:layout_constraintEnd_toStartOf="@+id/districtText"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/districtText"
        tools:ignore="MissingConstraints" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>